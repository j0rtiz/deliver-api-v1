{
  "name": "Conta",
  "plural": "Contas",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": ["create", "find"]
    },
    "Computed": {
      "properties": {
        "valorCorrigido": "valorCorrigido",
        "diasAtraso": "diasAtraso"
      }
    }
  },
  "hidden": ["ValorCorrigido", "DiasAtraso", "Regra"],
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "valorOriginal": {
      "type": "number",
      "required": true,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    },
    "ValorCorrigido": {
      "type": "number",
      "required": false,
      "default": null,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    },
    "dataVencimento": {
      "type": "date",
      "required": true
    },
    "dataPagamento": {
      "type": "date",
      "required": true
    },
    "DiasAtraso": {
      "type": "number",
      "required": false,
      "default": null
    }
  },
  "validations": [],
  "relations": {
    "atraso": {
      "type": "belongsTo",
      "model": "Atraso",
      "foreignKey": "Regra"
    }
  },
  "acls": [],
  "methods": {}
}

{
  "name": "Conta",
  "plural": "Contas",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": ["create", "find"]
    }
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "valorOriginal": {
      "type": "number",
      "required": true,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    },
    "valorCorrigido": {
      "type": "number",
      "required": false,
      "default": null,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    },
    "dataVencimento": {
      "type": "date",
      "required": true
    },
    "dataPagamento": {
      "type": "date",
      "required": true
    },
    "diasAtraso": {
      "type": "number",
      "required": false,
      "default": null
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
